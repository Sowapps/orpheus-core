{
	"name": "orpheus/orpheus-core",
	"description": "Orpheus Core",
	"type": "orpheus-library",
	"license": "MIT",
	"keywords": [
		"orpheus",
		"core"
	],
	"autoload": {
		"psr-4": {
			"Orpheus\\": "src/"
		},
		"files": [
			"src/_loader.php"
		]
	},
	"require": {
		"orpheus/orpheus-hook": "4.*"
	},
	"suggest": {
		"ext-fileinfo": "File features may require FileInfo"
	},
	"extra": {
		"orpheus-library": {
			"class": "Orpheus\\Core\\OrpheusCoreLibrary"
		}
	},
	"authors": [
		{
			"name": "Florent HAZARD",
			"email": "contact@sowapps.com",
			"homepage": "http://sowapps.com",
			"role": "Developer"
		}
	]
}
